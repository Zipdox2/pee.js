{
  "name": "pee.js",
  "version": "1.1.1",
  "description": "A javascript Library for leaking memory",
  "type": "module",
  "main": "pee.cjs",
  "module": "pee.js",
  "browser": "pee.js",
  "exports": {
    ".": {
      "require":  "./pee.cjs",
      "default": "./pee.js"
    }
  },
  "scripts": {
    "test:cjs": "jest --env node --testTimeout 15000 --moduleFileExtensions js --moduleFileExtensions cjs --testMatch '**/*.test.cjs'",
    "test:mjs": "NODE_OPTIONS=--experimental-vm-modules jest --env jsdom --transform '{}' --testTimeout 15000 --testMatch '**/*.test.js'",
    "test": "npm run test:cjs && npm run test:mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enbyautumn/pee.js.git"
  },
  "keywords": [
    "memory",
    "memory leak",
    "pee",
    "piss",
    "toilet",
    "leak",
    "garbage",
    "ram",
    "ram leak",
    "nodejs",
    "es6"
  ],
  "author": "Mint",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/enbyautumn/pee.js/issues"
  },
  "homepage": "https://github.com/enbyautumn/pee.js#readme",
  "dependencies": {
    "@canvas/image-data": "^1.0.0"
  },
  "devDependencies": {
    "jest": "^27.5.1"
  }
}
